,"column":10}},{"generated":{"line":90,"column":17},"source":"components/login-view/login-view.jsx","original":{"line":33,"column":5}},{"generated":{"line":91,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":50,"column":1}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from 'react';\nimport axios from 'axios';\n\nimport { BrowserRouter as Router, Route, Link } from 'react-router-dom';\n\nimport Container from 'react-bootstrap/Container';\nimport Form from 'react-bootstrap/Form';\nimport Button from 'react-bootstrap/Button';\n\nimport '../login-view/login-view.scss';\n\nexport function LoginView(props) {\n     const [username, setUsername] = useState('');\n     const [password, setPassword] = useState('');\n\n     const handleSubmit = (e) => {\n          e.preventDefault();\n          axios.post('https://fataleflix.herokuapp.com/login', {\n               Username: username,\n               Password: password\n          })\n               .then((response) => {\n                    const data = response.data;\n                    props.onLoggedIn(data);\n               })\n               .catch((e) => {\n                    console.log('no such user'),\n                         alert('make sure all login info is entered correctly!');\n               });\n     };\n\n\n     return (\n          <Container className=\"login-container\">\n               <Form>\n                    <Form.Group controlId=\"formBasicUsername\">\n                         <Form.Label>username: </Form.Label>\n                         <Form.Control type=\"text\" value={username} onChange={e => setUsername(e.target.value)} placeholder=\"enter username\" />\n                    </Form.Group>\n\n                    <Form.Group controlId=\"formBasicPassword\">\n                         <Form.Label>password: </Form.Label>\n                         <Form.Control type=\"password\" value={password} onChange={e => setPassword(e.target.value)} placeholder=\"enter password\" />\n                    </Form.Group>\n\n                    <Button variant=\"outline-danger\" size=\"sm\" block onClick={handleSubmit}>sign in</Button>\n               </Form>\n          </Container>\n     );\n}"},"lineCount":null}},"error":null,"hash":"7def3153fa3e1cf95bae304b489a3d63","cacheData":{"env":{}}}